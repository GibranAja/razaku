{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.rajaongkir.com/starter/:path*"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,POST,PUT,DELETE,OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, key"
        },
        {
          "key": "key",
          "value": "f5bbc8efa8f9454734a47ea7558f1737"
        }
      ]
    }
  ]
}